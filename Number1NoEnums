#include <iostream>
using namespace std;



int main(void)
{
    
    cout << (2 + 5) << endl;    
    cout << (6 - 4) << endl;   
    cout << (2 * 6) << endl;  
    cout << (8 / 4) << endl;  
    cout << (6 - 4) << endl;

//--------------------------------------------------//

    
    cout << (2 | 4) << endl;    
    cout << (6 & 4) << endl;  
    cout << (4 ^ 6) << endl; 
    cout << (8 << 1) << endl;  
    cout << (2 >> 1) << endl;   

//--------------------------------------------------//

    

    return 0;
}
